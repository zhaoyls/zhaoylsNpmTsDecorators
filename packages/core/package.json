{
  "name": "@zyl/core",
  "version": "1.0.0",
  "description": "npm 发布包测试代码，后续 TS 装饰器学习代码以及工具封装都会存在这里。",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "nodemon -e ts,tsx --exec ts-node --esm ./src/index.ts",
    "serve2": "nodemon --ext ts,tsx --exec ts-node --esm ./src/index.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.iife.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=14",
    "pnpm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "zhaoyls",
  "license": "ISC",
  "dependencies": {
    "@zyl/shared": "workspace:*",
    "chalk": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaoyls/zylsTsDecorators.git"
  }
}
